<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    tools:context="com.nsoftware.ipworks3ds.sdk.core.ClientWebChallengeListener">

    <com.nsoftware.ipworks3ds.sdk.core.SDKToolbarLayout
        android:id="@+id/sdkToolbarView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.nsoftware.ipworks3ds.sdk.core.MyWebView
        android:id="@+id/htmlWebView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>
